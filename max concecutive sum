class Solution
{
    
    public int findMaxConsecutiveOnes(int[] nums) 
    {
         int sum = 0;
        int totalmax= 0;
        
        for (int i = 0; i< nums.length; i++)
        {
            if(nums[i] == 1)
            {
                sum+=1;
                totalmax = Math.max(totalmax, sum);
            }
            else 
            {
                sum= 0;
            }
        }
        
        
        return totalmax;
    }
    
}
